{
    "name": "egarciahz/airam-framework",
    "license": "MIT",
    "description": "Airam is a web component oriented framework for the fast startup of web applications",
    "keywords": [
        "framework",
        "airam"
    ],
    "authors": [
        {
            "name": "Ezequiel Garcia",
            "email": "egarcia.informatic.ing@gmail.com"
        }
    ],
    "minimum-stability": "alpha",
    "prefer-stable": true,
    "bin": [
        "bin/airam"
    ],
    "autoload": {
        "psr-4": {
            "Airam\\": "src/Airam"
        },
        "files": [
            "src/Airam/functions.php",
            "src/Airam/Utils/functions.php",
            "src/Airam/Template/Lib/functions.php"
        ]
    },
    "require": {
        "php": ">=7.2.0",
        "zordius/lightncandy": "^1.2",
        "symfony/filesystem": "^5.1",
        "vlucas/phpdotenv": "^5.1",
        "php-di/php-di": "^6.2",
        "nikic/fast-route": "^1.3",
        "laminas/laminas-diactoros": "^2.3",
        "laminas/laminas-httphandlerrunner": "^1.2",
        "laminas/laminas-uri": "^2.7",
        "laminas/laminas-stratigility": "^3.2",
        "krowinski/http-status-codes": "^1.0",
        "middlewares/redirect": "^2.0",
        "doctrine/orm": "^2.7",
        "middlewares/whoops": "^2.0",
        "doctrine/annotations": "^1.10"
    }
}