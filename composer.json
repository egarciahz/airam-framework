{
    "name": "egarciahz/simplext-framework",
    "license": "MIT",
    "description": "simplext is a framework for php with a focus on giving web developers a quick start",
    "keywords": [
        "framework",
        "simplext"
    ],
    "authors": [
        {
            "name": "Ezequiel Garcia",
            "email": "egarcia.informatic.ing@gmail.com"
        }
    ],
    "minimum-stability": "alpha",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Core\\": "src",
            "App\\": "app"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "require": {
        "php": ">=7.2.0",
        "zordius/lightncandy": "^1.2",
        "symfony/filesystem": "^5.1",
        "vlucas/phpdotenv": "^5.1",
        "php-di/php-di": "^6.2",
        "nikic/fast-route": "^1.3",
        "laminas/laminas-diactoros": "^2.3",
        "laminas/laminas-httphandlerrunner": "^1.2",
        "laminas/laminas-uri": "^2.7",
        "laminas/laminas-stratigility": "^3.2",
        "krowinski/http-status-codes": "^1.0",
        "middlewares/redirect": "^2.0",
        "doctrine/orm": "^2.7",
        "middlewares/whoops": "^2.0"
    }
}